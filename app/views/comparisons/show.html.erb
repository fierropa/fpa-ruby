
<div id="wrapper" class="toggled">
  <!-- Sidebar -->
  <div id="sidebar-wrapper">
    
    <ul class="sidebar-nav">
      <li class="stripes">
        <%= image_tag "logo.png", title: "Fierro Public Affairs", class: "sidebar-logo", size: "150x150" %>
        <!-- <a href="#menu-toggle" id="menu-toggle" class="btn btn-default" style="font-size:2.5em;color:white;">â˜°</a> -->
      </li>
      <li class="sidebar-brand">
        <%= link_to "Fierro Public Affairs", root_url, class: 'w3-bar-item w3-button' %>
      </li>
      <li>
        <%= link_to "Another Comparison", new_comparison_url, class: 'w3-bar-item w3-button' %>
      </li>
    </ul>
  </div>
  <!-- /#sidebar-wrapper -->

  <!-- Page Content -->
  <div id="page-content-wrapper">
    <section>
      <div class="container-fluid">
        <div class="row">
          <div class="col-lg-12">
            <h1 id="side-by-side-comparison" class="text-center">Side By Side Comparison</h1>
            <div class="centered-image">
              <span></span>
              <%= image_tag(@comparison.result_image_url, title: "Comparison results.", class: "xy-centered", skip_pipeline: true) %>
            </div>
          </div>
        </div>
      </div>
      
      
    </section>
  </div>
</div><!-- /#page-content-wrapper -->

    <!-- Menu Toggle Script -->
    <script>
    $("#menu-toggle").click(function(e) {
        e.preventDefault();
        $("#wrapper").toggleClass("toggled");
    });
    </script>